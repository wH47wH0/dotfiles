filetype plugin indent on
set colorcolumn=120
set tabstop=4
set number
set autoindent
set shiftwidth=4
set showmatch
set textwidth=120
set background=dark
syntax enable

hi Comment    ctermfg=4 ctermbg=NONE cterm=NONE
hi Constant   ctermfg=3 ctermbg=NONE cterm=NONE
hi Identifier ctermfg=2 ctermbg=NONE cterm=bold
hi Statement  ctermfg=1 ctermbg=NONE cterm=NONE
hi PreProc    ctermfg=5 ctermbg=NONE cterm=NONE
hi Type       ctermfg=2 ctermbg=NONE cterm=NONE
hi Special    ctermfg=6 ctermbg=NONE cterm=NONE
hi Underlined ctermfg=4 ctermbg=NONE cterm=underline
hi Error      ctermfg=0 ctermbg=1    cterm=NONE
hi Todo       ctermfg=0 ctermbg=6    cterm=NONE         
hi LineNr     ctermfg=6 ctermbg=NONE cterm=NONE

